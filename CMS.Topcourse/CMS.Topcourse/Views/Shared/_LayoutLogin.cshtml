<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <title>
        Topcourse | Login
    </title>
    <meta name="description" content="Latest updates and statistic charts">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!--begin::Web font -->
    <script src="@Html.Raw(Topcourse.Utility.Config.UrlRoot)Scripts/webfont.js"></script>
    <script>
        WebFont.load({
            google: { "families": ["Poppins:300,400,500,600,700", "Roboto:300,400,500,600,700"] },
            active: function () {
                sessionStorage.fonts = true;
            }
        });
    </script>

    <!--end::Web font -->
    <!--begin::Base Styles -->
    <link href="@Html.Raw(Topcourse.Utility.Config.UrlRoot)assets/vendors/base/vendors.bundle.css" rel="stylesheet" type="text/css" />
    <link href="@Html.Raw(Topcourse.Utility.Config.UrlRoot)assets/demo/default/base/style.bundle.css" rel="stylesheet" type="text/css" />
    <!--end::Base Styles -->
    <link rel="shortcut icon" href="@Html.Raw(Topcourse.Utility.Config.UrlRoot)assets/demo/default/media/img/logo/favicon.ico" />

    @Scripts.Render("~/bundles/jquery")
</head>
<body class="m--skin- m-header--fixed m-header--fixed-mobile m-aside-left--enabled m-aside-left--skin-dark m-aside-left--offcanvas m-footer--push m-aside--offcanvas-default">


    <div class="m-grid m-grid--hor m-grid--root m-page">

        @RenderBody()
    </div>


    <!--begin::Base Scripts -->
    <script src="@Html.Raw(Topcourse.Utility.Config.UrlRoot)assets/vendors/base/vendors.bundle.js" type="text/javascript"></script>
    <script src="@Html.Raw(Topcourse.Utility.Config.UrlRoot)assets/demo/default/base/scripts.bundle.js" type="text/javascript"></script>
    <!--end::Base Scripts -->
    <!--begin::Page Snippets -->
    @*<script src="@Html.Raw(Topcourse.Utility.Config.UrlRoot)assets/snippets/pages/user/login.js" type="text/javascript"></script>*@
    <!--end::Page Snippets -->
    <script src="@Html.Raw(Topcourse.Utility.Config.UrlRoot)Scripts/sammy.js"></script>

    @Scripts.Render("~/bundles/common")
    @RenderSection("scripts", required: false)

</body>
</html>
